  "main": "index.js",
  "scripts": {
    "postinstall": "sh scripts/download-signed-utility.sh",
    "test": "npm run package && npm run verify",
    "build": "node build.js",
    "self-sign": "sh scripts/create-certificate.sh",
    "clean": "rm -f \"dist/fuse-fonts.zxp\"",
