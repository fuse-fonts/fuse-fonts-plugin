{
  "name": "fuse-fonts-plugin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node build.js",
    "self-sign": "ZXPSignCmd.exe -selfSignedCert US CA FuseFonts \"Fuse Fonts\" bravecoffee zxp.cert ",
    "clean": "rm -f \"dist/fuse-fonts.zxp\"",
    "generate": "npm run clean && ZXPSignCmd.exe -sign bin \"dist/fuse-fonts.zxp\" zxp.cert bravecoffee -tsa \"http://sha1timestamp.ws.symantec.com/sha1/timestamp\"",
    "package": "npm run build && npm run generate",
    "verify": "ZXPSignCmd.exe -verify dist/fuse-fonts.zxp"
  },
